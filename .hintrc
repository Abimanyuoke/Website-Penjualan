{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/html": "off",
    "axe/text-alternatives": [
      "off",
      {
        "frame-title": "off"
      }
    ],
    "disown-opener": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-mask-image",
          "-webkit-mask-repeat",
          "-webkit-mask-position",
          "-webkit-mask-size"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}